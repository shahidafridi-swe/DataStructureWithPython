   "source": [
    "def input_graph(node, edge):\n",
    "    arr = [[0]*node for _ in range(node)]\n",
    "\n",
    "    for i in range(edge): \n",
    "        x, y = map(int, input().split())\n",
    "        arr[x][y] = 1\n",
    "        arr[y][x] = 1\n",
    "\n",
    "    return arr"
   ]
